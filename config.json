{
    "source": ["figma/*.json"],
    "platforms": {
      "scss": {
        "transformGroup": "scss",
        "buildPath": "dist/scss/",
        "files": [
          {
            "destination": "_variables.scss",
            "format": "scss/variables"
          },
          {
            "destination": "_map.scss",
            "format": "scss/map-flat",
            "mapName": "qld-tokens"
          }
        ]
      }
    }
  }