{
  "source": ["figma/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "dist/scss/",
      "transforms": ["attribute/cti", "name/cti/kebab", "merge-static-variables"],
      "files": [
        {
          "destination": "_variables.scss",
          "format": "scss/variables-with-renaming"
        },
        {
          "destination": "_map.scss",
          "format": "scss/map-flat-with-renaming",
          "mapName": "qld-tokens"
        }
      ]
    }
  }
}
